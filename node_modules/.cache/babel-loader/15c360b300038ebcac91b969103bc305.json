A,EAAA;AACA,eAAA,KAAA,CAAA,UAAA,CAAA,MAAA,KAAA,EAAA,CAAA,YAAA;AACA,OAFA,CAAA;AAGA,WAAA,UAAA,CAAA,cAAA;AACA,UAAA,UAAA,GAAA,EAAA,CAAA,UAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,eAAA,KAAA,CAAA,UAAA,CAAA,MAAA,KAAA,EAAA,CAAA,YAAA,IAAA,KAAA,CAAA,UAAA,CAAA,IAAA,KAAA,EAAA,CAAA,YAAA;AACA,OAFA,CAAA;AAGA,MAAA,EAAA,CAAA,YAAA,GAAA,EAAA,CAAA,YAAA,GAAA,UAAA,GAAA,cAAA,CATA,CAUA;AACA;AACA;;AACA,aAAA,EAAA,CAAA,YAAA,GAAA,UAAA,GAAA,cAAA;AACA;AA3BA,GAZA;AAyCA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,aAAA;AACA,KAHA;AAIA,IAAA,UAJA,sBAIA,IAJA,EAIA;AACA,UAAA,IAAA,GAAA,IAAA,GAAA,EAAA;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,QAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,UAAA,CAAA,IAAA;AACA,OAFA;AAGA,UAAA,KAAA,GAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,OAAA,EAAA,KAAA;AACA,KAXA;AAYA,IAAA,cAZA,4BAYA;AACA,UAAA,SAAA,GAAA;AACA,uBAAA,IADA;AAEA,yBAAA;AAFA,OAAA;AAIA,aAAA,SAAA;AACA,KAlBA;AAmBA,IAAA,cAnBA,4BAmBA;AACA,UAAA,SAAA,GAAA;AACA,uBAAA,IADA;AAEA,yBAAA;AAFA,OAAA;AAIA,aAAA,SAAA;AACA;AAzBA,GAzCA;AAoEA,EAAA,OApEA,qBAoEA;AAAA;;AACA;AACA,SAAA,WAAA,GAFA,CAGA;;AACA,IAAA,SAAA,CAAA,WAAA,CAAA,kBAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,UAAA,CAAA,GAAA,GAAA,CAAA,MAAA,CAFA,CAGA;;AACA,MAAA,KAAA,CAAA,MAAA,GAAA,CAAA,CAAA,CAAA,QAAA,EAAA,CAAA,CAAA,SAAA,CAAA;AACA,KALA;AAMA;AA9EA,CAAA","sourcesContent":["<template>\n  <div id=\"app\" class=\"flex h-screen\">\n    <Loading :active.sync=\"isLoading\"></Loading>\n\n    <div class=\"w-3/12 h-screen overflow-y-scroll\">\n      <div class=\"bg-light p-4 sticky top-0 z-10\">\n        <div class=\"flex items-center mb-4\">\n          <label for=\"city\" class=\"mr-4\">縣市</label>\n          <select id=\"city\" class=\"block h-10 bg-white flex-auto\"\n            v-model=\"selectedCity\" @change=\"selectedZone = null\">\n            <option value=\"\" selected>=== 請選擇城市 ===</option>\n            <option :value=\"item\" v-for=\"item in county\" :key=\"item\">{{item}}</option>\n          </select>\n        </div>\n        <div class=\"flex items-center mb-4\">\n          <label for=\"zone\" class=\"mr-4\">地區</label>\n          <select id=\"zone\" class=\"block h-10 bg-white flex-auto\"\n            v-model=\"selectedZone\">\n            <option value=\"\" selected>=== 請選擇行政區 ===</option>\n            <option :value=\"item\" v-for=\"item in towns\" :key=\"item\">{{item}}</option>\n          </select>\n        </div>\n        <p class=\"text-right\">\n          取得口罩數量的藥局有 <span class=\"text-primary text-2xl\">{{ filterData.length }}</span> 家\n        </p>\n      </div>\n      <!-- 查詢結果 -->\n      <ul>\n        <li class=\"border-b-2 border-gray-200 p-4 hover:bg-blue-100\"\n          v-for=\"item in filterData\" :key=\"item.properties.id\">\n          <a :href=\"`https://www.google.com.tw/maps/search/${ item.properties.address }`\" target=\"_blank\">\n            <h1 class=\"text-xl font-medium mb-2\">{{ item.properties.name }}</h1>\n            <div class=\"text-grey flex items-center mb-2\">\n              <i class=\"map-icon mr-2 block h-4 w-4 bg-no-repeat bg-contain\"></i>\n              <span>{{ item.properties.address }}</span>\n            </div>\n            <div class=\"text-grey flex items-center mb-2\">\n              <i class=\"phone-icon mr-2 block h-4 w-4 bg-no-repeat\"></i>\n              <span>{{ item.properties.phone }}</span>\n            </div>\n            <div class=\"flex justify-center mb-2\">\n              <div class=\"text-center py-3 rounded-l rounded-r-none border-r w-1/2\"\n                :class=\"maskAdultClass()\"\n                v-if=\"item.properties.mask_adult === 0\">\n                <span class=\"mr-1\">成人口罩:</span>\n                <span>已售完</span>\n              </div>\n              <div class=\"text-center py-3 rounded-l rounded-r-none border-r w-1/2\n                bg-primary text-white\" v-else>\n                <span class=\"mr-1\">成人口罩:</span>\n                <span>{{ item.properties.mask_adult }}</span>\n              </div>\n              <div class=\"text-center py-3 rounded-r rounded-l-none w-1/2\"\n                :class=\"maskChildClass()\"\n                v-if=\"item.properties.mask_child === 0\">\n                <span class=\"mr-1\">兒童口罩:</span>\n                <span>已售完</span>\n              </div>\n              <div class=\"text-center py-3 rounded-r rounded-l-none border-r w-1/2\n                bg-primary text-white\" v-else>\n                <span class=\"mr-1\">兒童口罩:</span>\n                <span>{{ item.properties.mask_child }}</span>\n              </div>\n            </div>\n            <div class=\"text-sm text-grey\">\n              更新時間: {{ item.properties.updated === '' ? '無' : item.properties.updated }}\n            </div>\n          </a>\n        </li>\n      </ul>\n    </div>\n    <VueLeaflet :update-map=\"searchResult\" />\n  </div>\n</template>\n\n<script>\n/* eslint-disable arrow-body-style */\n// import { latLng } from 'leaflet';\nimport VueLeaflet from './components/VueLeaflet.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    VueLeaflet,\n  },\n  data() {\n    return {\n      selectedCity: '高雄市',\n      selectedZone: '三民區',\n      searchResult: [],\n    };\n  },\n  computed: {\n    isLoading() { // 更新 Loading 狀態\n      return this.$store.state.isLoading;\n    },\n    pharmacies() { // 健保署資料\n      return this.$store.state.pharmacies;\n    },\n    county() { // 縣市\n      return this.$store.state.county;\n    },\n    towns() { // 縣市的地區\n      return this.$store.state.towns;\n    },\n    filterData() {\n      const vm = this;\n      const filterCityData = vm.pharmacies.filter((value) => {\n        return value.properties.county === vm.selectedCity;\n      });\n      this.filterTown(filterCityData);\n      const filterData = vm.pharmacies.filter((value) => {\n        return value.properties.county === vm.selectedCity && value.properties.town === vm.selectedZone;\n      });\n      vm.searchResult = vm.selectedZone ? filterData : filterCityData;\n      // console.log('result', vm.searchResult);\n      // console.log('city', vm.selectedCity);\n      // console.log('zone', vm.selectedZone);\n      return vm.selectedZone ? filterData : filterCityData;\n    },\n  },\n  methods: {\n    getPharmacy() {\n      this.$store.dispatch('getPharmacy');\n    },\n    filterTown(data) {\n      const zone = new Set();\n      data.forEach((value) => {\n        zone.add(value.properties.town);\n      });\n      const zones = Array.from(zone);\n      this.$store.dispatch('towns', zones);\n    },\n    maskAdultClass() {\n      const maskAdult = {\n        'bg-gray-300': true,\n        'text-gray-500': true,\n      };\n      return maskAdult;\n    },\n    maskChildClass() {\n      const maskChild = {\n        'bg-gray-300': true,\n        'text-gray-500': true,\n      };\n      return maskChild;\n    },\n  },\n  mounted() {\n    // 取得藥局資料\n    this.getPharmacy();\n    // 取得目前所在位置經緯度\n    navigator.geolocation.getCurrentPosition((pos) => {\n      // console.log(pos);\n      const p = pos.coords;\n      // this.center = latLng(p.latitude, p.longitude);\n      this.center = [p.latitude, p.longitude];\n    });\n  },\n};\n</script>\n<style lang=\"scss\">\n  @import \"~leaflet.markercluster/dist/MarkerCluster.css\";\n  @import \"~leaflet.markercluster/dist/MarkerCluster.Default.css\";\n  .map-icon {\n    background-image: url('./assets/images/location.svg');\n  }\n  .phone-icon {\n    background-image: url('./assets/images/phone.svg');\n  }\n</style>\n"],"sourceRoot":"src"}]}